{
  "name": "tm-knowledge-store",
  "version": "1.0.0",
  "description": "Stores entities and vocuabularies",
  "main": "dist/src/app.js",
  "scripts": {
    "start": "node dist/src/app.js",
    "debug": "DEBUG=express:* node build/app.js",
    "test": "npx jest --passWithNoTests",
    "lint": "npx eslint",
    "generate": "npx openapi-generator-cli generate -g typescript-node -o src/generated -i ../openapi.yaml -c openapitools.json",
    "generate:build": "npm run generate && npm run build",
    "build": "npx tsc src/app.ts",
    "build:start": "npm run generate:build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TailoredMediaProject/tm-knowledge-store.git"
  },
  "author": "Redlink GmbH",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TailoredMediaProject/tm-knowledge-store/issues"
  },
  "homepage": "https://github.com/TailoredMediaProject/tm-knowledge-store#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^4.2.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.4.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
