{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.0",
    "generators": {
      "TN": {
        "generatorName": "typescript-angular",
        "output": "./src/generated",
        "glob": "../openapi.yaml",
        "globalProperty": {
          "models": "Entity:TagType:Id:DateTime:Pageable:Vocabulary"
        },
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": "true"
        }
      }
    }
  }
}
